{
  "name": "reactnativestarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android:clean": "cd android && gradlew clean && cd ..",
    "script:lint": "tslint -c ./tslint.json --project ./tsconfig.json",
    "script:pretty": "prettier --write src/**/*.ts src/**/*.tsx",
    "check": "npm run script:pretty && npm run script:lint",
    "precommit": "npm run check",
    "test": "jest"
  },
  "dependencies": {
    "@types/react-router": "^4.4.3",
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-thunk": "^2.1.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-native": "^0.57.18",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "23.6.0",
    "husky": "^1.3.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.0",
    "prettier": "^1.15.3",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "16.6.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  }
}
